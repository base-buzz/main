  "scripts": {
    "dev": "next dev -p 3333",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=8192' pnpm --filter=shadcn build && next build",
    "seed:tasks": "tsx --tsconfig ./tsconfig.scripts.json ./app/(app)/examples/tasks/data/seed.ts",
    "start": "next start -p 3333",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start -p 3333",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "generate-mock-data": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/generate-mock-data.ts",
    "reload": "bash -c '(lsof -ti :3333 | xargs kill -9 || true) && pnpm dev 2>&1 | ts \"[%Y-%m-%d %H:%M:%S]\" | tee server.log'",
    "test:wallet": "tsx test-wallet-auth.ts"
